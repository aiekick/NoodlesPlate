withCredentials([string(credentialsId: 'PIPELINES_REPO', variable: 'checkout_url')]) {
    library(
        identifier: 'origin@master', 
        retriever: modernSCM(
            [
                $class: 'GitSCMSource',
                remote: checkout_url,
                credentialsId: 'GITHUB_ACCESS_TOKEN'
            ]
        )
    )
}

pipeline_github_ndp("WINDOWS")
